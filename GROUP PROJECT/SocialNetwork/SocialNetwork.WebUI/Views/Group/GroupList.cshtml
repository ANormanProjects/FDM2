@model IEnumerable<SocialNetwork.WebUI.Models.GroupViewModels>
@{
    ViewBag.Title = "Your Groups";
}

<h2 class="text-center">Your Groups</h2>

<div class="col-xs-4 col-md-10">
    <table class="table table-hover">
        @foreach (SocialNetwork.WebUI.Models.GroupViewModels group in Model)
        {
            @Html.Partial("_GroupPreview", group)
        }
    </table>
</div>

