@model SocialNetwork.WebUI.Models.ProfilePageViewModel
@{
    ViewBag.Title = "ProfilePage";
}

<div class="jumbotron">
    <img src="~/Content/cover.jpg" alt="coverimg" class="under" style="width:100%;height:300px" />
    <img src="~/Content/@Model.user.username-placeholderprofile.jpg" alt="profileimg" class="over" style="width:200px;height:200px" />
</div>

<div class="UserPosts">
    @foreach (SocialNetwork.DataAccess.UserPost post in Model.userpost)
    {
        @Html.Partial("_UserPost", new SocialNetwork.WebUI.Models.UserPostViewModel() { post = post })
    }
</div>
